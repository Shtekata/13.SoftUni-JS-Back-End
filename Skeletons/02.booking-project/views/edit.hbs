 <section id="viewAddhotel">
    <h2>Edit existing hotel</h2>
    <form id="formAddhotel" method="POST">
        <label for="hotel">Hotel name:</label>
        <input type="text" id="hotel" name="hotel" value="{{x.name}}">
        <label for="city">City:</label>
        <input type="text" id="city" name="city" value="{{x.city}}">
        <label for="free-rooms">Free rooms:</label>
        <input type="number" id="free-rooms" name="free-rooms"
            value="{{x.freeRooms}}">
        <label for="imgUrl">Image:</label>
        <input type="text" id="imgUrl" name="imgUrl"
            value="{{x.imageUrl}}">
        <input type="submit" class="create" value="Add">
    </form>
</section>